import { Duration } from "../timer/timer.ht"

external class FutureUtils {
  static fun delayed(
    duration: Duration,
    callback: Function
  ) -> Future

  static fun execute(
    futures: List, // List of futures
    { delay: Duration }
  ) -> Future

  static fun chain(
    futures: List, // List of callbacks returning futures
    { delay: Duration }
  ) -> Future
}

export { FutureUtils }